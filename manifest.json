{
	"name": "Atlas Obscura New Tab",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Get travel inspiration from Atlas Obscura in every new tab.",
	"homepage_url": "http://atlasobscura.com",
	"icons": {
	"16": "icons/icon16.png",
	"48": "icons/icon48.png",
	"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"chrome_url_overrides": {
	"newtab": "index.html"
	},
	"permissions": ["<all_urls>"]
}